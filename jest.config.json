{
  "collectCoverage": true,
  "coverageReporters": ["json-summary", "text"],
  "modulePathIgnorePatterns": ["<rootDir>/accounts/", "<rootDir>/testAccounts/"],
  "coverageThreshold": {
    "global": {
      "branches": 30,
      "functions": 70,
      "lines": 80,
      "statements": 80
    }
  }
}
